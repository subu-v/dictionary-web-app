h: 74rem;\n    margin: 0 auto;\n  }\n  &__switcher-container {\n    @include flex(row, space-between, center, 0);\n    margin-bottom: 4.8rem;\n  }\n  &__logo {\n    width: 3.6rem;\n    height: 3.6rem;\n  }\n\n  &__switchers {\n    @include flex(row, center, center, 6.4rem);\n  }\n\n  &__font-switcher {\n    @include flex(row, center, center, 2rem);\n    cursor: pointer;\n    position: relative;\n    & > ul,\n    & > ul > ul {\n      list-style: none;\n    }\n    & > ul {\n      position: relative;\n    }\n\n    &::after {\n      content: \"\";\n      position: absolute;\n      top: -5px;\n      right: -28px;\n      width: 2px;\n      height: 36px;\n      background-color: rgb(233, 233, 233);\n      box-shadow: 0 7.5px 7.5px rgba(0, 0, 0, 0.25);\n    }\n  }\n\n  &__font-switcher:hover &__font-items-dropdown {\n    opacity: 100;\n    visibility: visible;\n    translate: 0 7.5px;\n  }\n\n  &__font-desc {\n    font-size: 1.8rem;\n  }\n\n  &__font-items-dropdown {\n    opacity: 0;\n    visibility: hidden;\n    translate: 0 -7.5px;\n    transition: all 0.3s ease-in;\n    background-color: #fff;\n    box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.05);\n    border-radius: 12.5px;\n    width: 18.3rem;\n    padding: 2.4rem;\n    position: absolute;\n    top: 36px;\n    right: -36px;\n  }\n\n  &__font-item {\n    cursor: pointer;\n    font-size: 1.8rem;\n    transition: color 0.25s ease;\n    &:not(:last-child) {\n      margin-bottom: 1.8rem;\n    }\n    &:hover {\n      color: var(--clr-primary);\n    }\n  }\n\n  &__font-switcher-icon {\n    width: 1.6rem;\n    height: 1.6rem;\n  }\n\n  &__theme-switcher {\n    @include flex(row, center, center, 4.8rem);\n  }\n\n  &__toggle-box {\n    position: relative;\n  }\n  &__toggle-slider {\n    cursor: pointer;\n    background-color: #838383;\n    box-shadow: 0 5px 5px rgba(0, 0, 0, 0.25);\n    border-radius: 50px;\n    display: inline-block;\n\n    position: absolute;\n    top: -3px;\n    left: -12px;\n    width: 4.8rem;\n    height: 2.4rem;\n\n    transition: background 0.25s ease;\n\n    &:hover {\n      background-color: var(--clr-primary);\n    }\n\n    &::after {\n      content: \"\";\n      position: absolute;\n      top: 2.5px;\n      left: 3px;\n      width: 1.8rem;\n      height: 1.8rem;\n      background-color: white;\n      border-radius: 50%;\n    }\n  }\n\n  &__theme-switcher-icon {\n    width: 2.4rem;\n    height: 2.4rem;\n    filter: drop-shadow(0.5px 3px 2px rgba(0, 0, 0, 0.35));\n  }\n\n  &__input {\n    background-color: var(--clr-input-bg);\n    background-image: url(\"../../assets/images/icon-search.svg\");\n    background-repeat: no-repeat;\n    background-position: 97% 22px;\n    border: 1.5px solid transparent;\n    border-radius: 15px;\n    color: var(--clr-text);\n    cursor: pointer;\n    caret-color: var(--clr-primary);\n    font-size: 1.8rem;\n    outline: none;\n    padding: 2.4rem;\n    width: 100%;\n    height: 6.4rem;\n    transition: border 0.25s ease;\n\n    &:focus {\n      border: 1.5px solid var(--clr-primary);\n    }\n\n    &::placeholder {\n      color: rgb(131, 131, 131);\n      font-size: 1.8rem;\n      font-weight: 500;\n    }\n  }\n}\n","sass/abstracts/_mixins.scss":"@mixin flex($direction, $justify-content, $align-items, $gap) {\n  display: flex;\n  flex-direction: $direction;\n  justify-content: $justify-content;\n  align-items: $align-items;\n  gap: $gap;\n}\n"},"lineCount":null}},"error":null,"hash":"522063d4819eaf1402139ef56f67205a","cacheData":{"env":{}}}